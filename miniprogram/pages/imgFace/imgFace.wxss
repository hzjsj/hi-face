.detect-face-page {
  height: 100vh;
  box-sizing: border-box;
  padding-top: 88rpx;
  background-image: linear-gradient(180deg, rgba(28, 37, 50, .45) 0, rgba(28, 37, 50, .7) 76%, #1c2532 100%);
  background-repeat: no-repeat
}

.page-title {
  padding-left: 60rpx;
  font-size: 48rpx;
  font-weight: 600;
  color: #fff;
  line-height: 88rpx
}

.label-list {
  margin-top: 40rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  min-height: 36rpx
}

.label-item {
  font-size: 24rpx;
  line-height: 1.5;
  color: #fff;
  padding: 0 10rpx;
  border: 2rpx dotted #fff;
  margin: 0 10rpx
}

.image-wrap {
  margin: 20rpx auto 0;
  width: 600rpx
}

.to-choose {
  border: 2rpx dashed #efefef;
  width: 596rpx;
  height: 596rpx;
  margin: 0 auto;
  background: url(https://7376-svip9-1258873690.tcb.qcloud.la/images/photoAdd.png?sign=83b165f14cc5f57536a84b0755c4cd47&t=1588558330) no-repeat center/auto 300rpx
}

.image-selected {
  width: 600rpx;
  height: 600rpx
}

.shape-wrap {
  position: relative
}

.shape-item {
  position: absolute
}

.shape-area {
  height: 100%;
  box-shadow: inset 0 0 0 2rpx #4ec9b0
}

.face-line {
  position: absolute;
  opacity: .9;
  width: 25%;
  height: 25%
}

.face-line.left-top {
  left: 0;
  top: 0;
  border-left: #61e6bd 6rpx solid;
  border-top: #61e6bd 6rpx solid
}

.face-line.right-top {
  right: 0;
  top: 0;
  border-right: #61e6bd 6rpx solid;
  border-top: #61e6bd 6rpx solid
}

.face-line.left-bottom {
  left: 0;
  bottom: 0;
  border-left: #61e6bd 6rpx solid;
  border-bottom: #61e6bd 6rpx solid
}

.face-line.right-bottom {
  right: 0;
  bottom: 0;
  border-right: #61e6bd 6rpx solid;
  border-bottom: #61e6bd 6rpx solid
}

.shape-item-active {
  z-index: 1
}

.shape-item-active .shape-area {
  box-shadow: inset 0 0 0 2rpx #ed4ba6
}

.shape-item-active .face-line {
  border-color: #ed4ba6
}

.shape-desc {
  position: absolute;
  font-size: 28rpx;
  top: -1%;
  width: 210rpx;
  background-color: #61e6bd;
  padding: 28rpx 20rpx;
  white-space: nowrap
}

.shape-desc.in-right {
  left: 103%
}

.shape-desc.in-left {
  right: 103%
}

.image-tips {
  margin-top: 20rpx;
  font-size: 28rpx;
  line-height: 1.5;
  color: #fff;
  text-align: center
}

.cut-wrap {
  white-space: nowrap
}

.cut-item {
  margin-top: 20rpx;
  width: 120rpx;
  height: 120rpx;
  box-shadow: 0 0 0 2rpx #4ec9b0;
  margin-right: 18rpx;
  margin-left: 2rpx;
  background: #fff
}

.cut-item-active {
  box-shadow: 0 0 0 2rpx #ed4ba6
}

.main-button {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  background: #3172ee;
  font-size: 36rpx;
  line-height: 120rpx;
  font-weight: 600;
  color: #fff;
  text-align: center;
  letter-spacing: 6rpx
}

.main-button:before {
  content: '';
  display: inline-block;
  width: 54rpx;
  height: 54rpx;
  background: url(https://7376-svip9-1258873690.tcb.qcloud.la/images/buttonAdd.png?sign=c46627ec2ccc50abb974f0e6a8e84479&t=1588558572) no-repeat;
  background-size: contain;
  vertical-align: middle;
  margin: -6rpx 16rpx 0 0
}